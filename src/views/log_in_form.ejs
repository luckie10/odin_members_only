<form method="POST">
  <div>
    <label for="username">Username:</label>
    <input type="text" name="username" value="<%= username || '' %>"  />
  </div>
  <div>
    <label for="password">Password:</label>
    <input type="password" name="password" value="" />
  </div>
  <div>
    <button type="submit">Log in</button>
  </div>
</form>
